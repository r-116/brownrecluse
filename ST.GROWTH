print("Enter Current & Past Populations.")
CP = int(input("Current Population:"))
PP = int(input("Past Population:"))
GR = float(((CP-PP)/PP)) #Growth Rate
print("Current Growth Rate for Population is:", GR)
print("Enter Period.")
PE = int(input("Periodization:"))

for year in range(1, PE + 1):
    print( year, "\t", CP)
    CP = CP + (GR * CP)

